{
    "name": "metinbaris/inventory-bundle",
    "description": "Updates your Symfony apps MYSQL database with a given CSV file",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "7.0.*",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "MetinBaris\\InventoryBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetinBaris\\InventoryBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Metin Baris"
        }
    ]
}